{
  "name": "kedata",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "browserify client-src/app.js -o public/bundle.js",
    "watch": "watchify client-src/app.js  -o public/bundle.js --debug --verbose"
  },
  "dependencies": {
    "async": "^2.0.0-rc.4",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "csv-parser": "^1.9.3",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "angular": "^1.5.5",
    "angular-local-storage": "^0.2.7",
    "angular-ui-bootstrap": "^1.3.2",
    "angular-ui-router": "^0.2.18",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "es6ify": "^1.6.0",
    "jquery": "^2.2.3",
    "watchify": "^3.7.0"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "jvectormap": "./client-src/lib/jquery-jvectormap-2.0.3.min.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends":[
        "jquery:jQuery"
      ]
    },
    "jvectormap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
